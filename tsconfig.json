{
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true, 
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"]
}